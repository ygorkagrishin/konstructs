gostHeading()
    position absolute 
    top -1000px
    left -1000px
    width 0 
    height 0
    overflow hidden
clearfix()
    content ""
    display block
    clear both
buttonReset()
    border none
    margin 0
    padding 0
    width auto
    overflow visible
    background transparent
    color inherit
    font inherit
    line-height normal
    -webkit-font-smoothing inherit
    -moz-osx-font-smoothing inherit
    -webkit-appearance none
    
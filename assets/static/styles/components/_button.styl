.btn__menu_type_open
    buttonReset()
    span 
        display block
        width 2rem
        height .33rem 
        background-color #fff
        &:not(:last-child)
            margin-bottom .33rem
.btn__menu_type_close
    colorLink = #798184

    buttonReset()
    position absolute
    top 2rem
    right 1.5rem
    z-index 9999999
    color colorLink
    &:hover
        color primaryBlack